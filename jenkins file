pipeline {
  agent any
  stages {
    stage ('checkout') {
      steps {
	    echo 'cloning from git'
        checkout scm      
	  }
    }
  }
    stage ('Build') {
        echo 'building gradle'
        steps {
        sh 'whoami'
        sh 'gradle dileep'
       }
    }
}
			
    
    
    
    
    
    
    
    
 
	    
